{
  "displayName": "Progress Bar",
  "defaultWidth": 300,
  "defaultHeight": 8,
  "resizeY": true,
  "resizeX": true,
  "components": "./index.js",
  "icon": "./example-thumbnail.png",
  "props": [
    {
      "name": "progressValue",
      "displayName": "Progress Value",
      "type": "number",
      "default": 0
    },
    {
      "name": "maxValue",
      "displayName": "Max Value",
      "type": "number",
      "default": 100
    },
    {
      "name": "animationSpeed",
      "displayName": "Animation Speed",
      "type": "number",
      "default": 12,
      "control": { "type": "slider", "max": 100, "min": 0}
    }, 
    {
      "name": "animationBounciness",
      "displayName": "Animation Bounciness",
      "type": "number",
      "default": 8,
      "control": { "type": "slider", "max": 50, "min": 0}
    },
    {
      "name": "backgroundColor",
      "displayName": "Background Color",
      "type": "color",
      "default": "@primary"
    },
    {
      "name": "progressColor",
      "displayName": "Progress Color",
      "type": "color",
      "default": "@primaryLight"
    },
    {
      "name": "rounding",
      "displayName": "Rounding",
      "type": "number",
      "default": 0,
      "control": { "type": "slider", "max": 100, "min": 0}
    },
    {
      "name": "direction",
      "displayName": "Direction",
      "type": "number",
      "default": 0,
      "control": {
        "type": "menu",
        "options": [
          { "label": "Left to Right", "value": 0 },
          { "label": "Right to Left", "value": 1 },
          { "label": "Top to Bottom", "value": 2 },
          { "label": "Bottom to Top", "value": 3 }
        ]
      }
    }
  ]
}