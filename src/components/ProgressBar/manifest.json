{
  "displayName": "Progress Bar",
  "defaultWidth": 300,
  "defaultHeight": 4,
  "resizeY": true,
  "resizeX": true,
  "components": "./index.js",
  "icon": "./example-thumbnail.png",
  "props": [
    {
      "name": "progressValue",
      "displayName": "Progress Value",
      "type": "number",
      "default": 0
    },
    {
      "name": "maxValue",
      "displayName": "Max Value",
      "type": "number",
      "default": 100
    },
    {
      "name": "color",
      "displayName": "Color",
      "type": "color",
      "default": "@primary"
    },
    {
      "name": "backgroundColor",
      "displayName": "Background Color",
      "type": "color",
      "default": "@primaryLight"
    },
    {
      "name": "rounding",
      "displayName": "Rounding",
      "type": "number",
      "default": 0,
      "control": { "type": "slider", "max": 100, "min": 0}
    }
  ]
}